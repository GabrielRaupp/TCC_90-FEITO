label htmlFor=\"password\">Senha:</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n            required\n          />\n        </div>\n        <button type=\"submit\" className={styles.submitButton}>Entrar</button>\n      </form>\n      <button className={styles.resetPasswordButton} onClick={handlePasswordReset}>\n        Redefinir Senha\n      </button>\n    </div>\n  );\n};\n\nexport default Login;\n","/home/redes/Gabriel_Raupp/Test_Front/src/components/pages/Cadastro.js",[],"/home/redes/Gabriel_Raupp/Test_Front/src/components/pages/NewHorario.js",[],"/home/redes/Gabriel_Raupp/Test_Front/src/components/horario/HorarioForm.js",[],"/home/redes/Gabriel_Raupp/Test_Front/src/components/pages/ForgotPassword.js",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":8,"column":10,"nodeType":"107","messageId":"108","endLine":8,"endColumn":20},{"ruleId":"105","severity":1,"message":"109","line":9,"column":10,"nodeType":"107","messageId":"108","endLine":9,"endColumn":26},{"ruleId":"105","severity":1,"message":"110","line":28,"column":13,"nodeType":"107","messageId":"108","endLine":28,"endColumn":17},"no-native-reassign",["111"],"no-negated-in-lhs",["112"],"no-unused-vars","'isLoggedIn' is assigned a value but never used.","Identifier","unusedVar","'loggedInUsername' is assigned a value but never used.","'data' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]