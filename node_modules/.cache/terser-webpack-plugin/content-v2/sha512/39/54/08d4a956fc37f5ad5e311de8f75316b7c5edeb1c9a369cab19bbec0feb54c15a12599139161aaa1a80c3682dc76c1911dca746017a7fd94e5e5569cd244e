\n      console.log('horarioPayload:', horarioPayload); \n\n      if (horarioData) {\n        response = await axios.put(`/horarios/${horarioData._id}`, horarioPayload);\n      } else {\n        response = await axios.post('/horarios', horarioPayload);\n      }\n\n      \n      setName('');\n      setHorarios('');\n      setCategory('');\n      setErrorMessage(''); \n\n      if (onSubmitSuccess) {\n        onSubmitSuccess(response.data); \n      }\n    } catch (error) {\n      console.error('Erro ao salvar o horário:', error);\n      setErrorMessage('Erro ao salvar o horário. Tente novamente.');\n    }\n  };\n\n  return (\n    <div className={styles.form}>\n      <h2>{horarioData ? 'Editar Horário' : 'Cadastrar Horário'}</h2>\n      <form onSubmit={handleSubmit}>\n        <div>\n          <label htmlFor=\"name\">Nome:</label>\n          <input\n            type=\"text\"\n            id=\"name\"\n            value={name}\n            onChange={(e) => setName(e.target.value)}\n            placeholder=\"Digite o nome\"\n            required\n          />\n        </div>\n        <div>\n          <label htmlFor=\"horarios\">Horários:</label>\n          <input\n            type=\"datetime-local\"\n            id=\"horarios\"\n            value={horarios}\n            onChange={(e) => setHorarios(e.target.value)}\n            required\n          />\n        </div>\n        <div>\n  <label htmlFor=\"category\">Categoria:</label>\n  <select\n    id=\"category\"\n    value={category}\n    onChange={(e) => setCategory(e.target.value)}\n    required\n  >\n    <option value=\"\">Selecione uma categoria</option>\n    <option value=\"Trabalho\">Trabalho</option>\n    <option value=\"Prova\">Prova</option>\n    <option value=\"Orientação\">Orientação</option>\n    <option value=\"Co-Orientação\">Co-Orientação</option>\n  </select>\n</div>\n        {errorMessage && <p className={styles.error}>{errorMessage}</p>} \n        <button type=\"submit\">{horarioData ? 'Atualizar Horário' :