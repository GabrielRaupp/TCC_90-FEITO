ta do servidor\n\n      // Limpa os campos aps o envio\n      setName('');\n      setHorarios('');\n      setCategory('');\n      setErrorMessage(''); \n\n      if (onSubmitSuccess) {\n        onSubmitSuccess(response.data); \n      }\n    } catch (error) {\n      console.error('Erro ao salvar o horrio:', error);\n      setErrorMessage('Erro ao salvar o horrio. Tente novamente.');\n    }\n  };\n\n  return (\n    <div className={styles.form}>\n      <h2>{horarioData ? 'Editar Horrio' : 'Cadastrar Horrio'}</h2>\n      <form onSubmit={handleSubmit}>\n        <div>\n          <label htmlFor=\"name\">Nome:</label>\n          <input\n            type=\"text\"\n            id=\"name\"\n            value={name}\n            onChange={(e) => setName(e.target.value)}\n            placeholder=\"Digite o nome\"\n            required\n          />\n        </div>\n        <div>\n          <label htmlFor=\"horarios\">Horrios:</label>\n          <input\n            type=\"datetime-local\" // Campo para datas e horrios\n            id=\"horarios\"\n            value={horarios}\n            onChange={(e) => setHorarios(e.target.value)}\n            required\n          />\n        </div>\n        <div>\n          <label htmlFor=\"category\">Categoria:</label>\n          <select\n            id=\"category\"\n            value={category}\n            onChange={(e) => setCategory(e.target.value)}\n            required\n          >\n            <option value=\"\">Selecione uma categoria</option>\n            <option value=\"Trabalho\">Trabalho</option>\n            <option value=\"Prova\">Prova</option>\n            <option value=\"Orientao\">Orientao</option>\n            <option value=\"Co-Orientao\">Co-Orientao</option>\n          </select>\n        </div>\n\n        {errorMessage && <p className={styles.error}>{errorMessage}</p>} {/* Mensagem de erro */}\n        <button type=\"submit\">{horarioData ? 'Atualizar Horrio' : 'Salvar Horrio'}</button> {/* Boto */}\n      </form>\n    </d